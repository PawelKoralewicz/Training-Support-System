{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "unique": false,
      "configurable": false,
      "minLength": 3,
      "required": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "lastName": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "required": true
    },
    "rpeCharts": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rpe.rpe",
      "mappedBy": "user"
    },
    "workoutPlans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::users-workout-plan.users-workout-plan",
      "mappedBy": "user"
    },
    "permissions": {
      "type": "enumeration",
      "enum": [
        "user",
        "admin"
      ],
      "default": "user"
    },
    "personalInfo": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::personal-info.personal-info",
      "inversedBy": "user"
    },
    "meals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::users-meal.users-meal",
      "mappedBy": "user"
    }
  }
}
